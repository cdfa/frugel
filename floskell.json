{
    "style": "cramer",
    "extensions": ["OverloadedLabels"],
    "fixities": ["infixr 8 .:."],
    "formatting": {
        "op": {
            ",": {
                "force-linebreak": false,
                "spaces": "after",
                "linebreaks": "before"
            },
            "=": {
                "force-linebreak": false,
                "spaces": "both",
                "linebreaks": "before"
            },
            "@": {
                "force-linebreak": false,
                "spaces": "none",
                "linebreaks": "none"
            },
            "default": {
                "force-linebreak": false,
                "spaces": "both",
                "linebreaks": "before"
            },
            "-> in expression": {
                "force-linebreak": false,
                "spaces": "both",
                "linebreaks": "after"
            },
            ". in type": {
                "force-linebreak": false,
                "spaces": "after",
                "linebreaks": "after"
            },
            "$": {
                "force-linebreak": false,
                "spaces": "both",
                "linebreaks": "before"
            },
            "record in pattern": {
                "force-linebreak": false,
                "spaces": "none",
                "linebreaks": "none"
            },
            "record": {
                "force-linebreak": false,
                "spaces": "after",
                "linebreaks": "none"
            }
        },
        "group": {
            "$(": {
                "force-linebreak": false,
                "spaces": "none",
                "linebreaks": "none"
            },
            "[ in pattern": {
                "force-linebreak": false,
                "spaces": "none",
                "linebreaks": "after"
            },
            "[p|": {
                "force-linebreak": false,
                "spaces": "none",
                "linebreaks": "none"
            },
            "default": {
                "force-linebreak": false,
                "spaces": "both",
                "linebreaks": "after"
            },
            "( in other": {
                "force-linebreak": false,
                "spaces": "both",
                "linebreaks": "after"
            },
            "[ in type": {
                "force-linebreak": false,
                "spaces": "none",
                "linebreaks": "none"
            },
            "[|": {
                "force-linebreak": false,
                "spaces": "none",
                "linebreaks": "none"
            },
            "* in type": {
                "force-linebreak": false,
                "spaces": "none",
                "linebreaks": "after"
            },
            "* in pattern": {
                "force-linebreak": false,
                "spaces": "none",
                "linebreaks": "after"
            },
            "(": {
                "force-linebreak": false,
                "spaces": "none",
                "linebreaks": "after"
            },
            "[d|": {
                "force-linebreak": false,
                "spaces": "none",
                "linebreaks": "none"
            },
            "[t|": {
                "force-linebreak": false,
                "spaces": "none",
                "linebreaks": "none"
            }
        },
        "layout": {
            "infix-app": "try-oneline",
            "if": "try-oneline",
            "import-spec-list": "flex",
            "con-decls": "try-oneline",
            "declaration": "try-oneline",
            "app": "try-oneline",
            "let": "try-oneline",
            "record": "try-oneline",
            "type": "try-oneline",
            "export-spec-list": "try-oneline",
            "list-comp": "try-oneline"
        },
        "penalty": {
            "overfull": 10,
            "indent": 1,
            "overfull-once": 200,
            "max-line-length": 80,
            "linebreak": 100
        },
        "indent": {
            "deriving": 4,
            "if": "align-or-indent-by 4",
            "let-binds": "align-or-indent-by 4",
            "import-spec-list": "align-or-indent-by 4",
            "onside": 4,
            "where": 2,
            "typesig": "align-or-indent-by 4",
            "do": "indent-by 4",
            "app": "align-or-indent-by 4",
            "case": "indent-by 4",
            "let-in": "align-or-indent-by 4",
            "where-binds": "indent-by 2",
            "let": "align-or-indent-by 4",
            "export-spec-list": "indent-by 4",
            "multi-if": "align-or-indent-by 4",
            "class": "indent-by 4"
        },
        "align": {
            "let-binds": false,
            "where": false,
            "matches": false,
            "limits": [
                10,
                25
            ],
            "case": false,
            "import-module": false,
            "import-spec": true,
            "class": false,
            "record-fields": false
        },
        "options": {
            "sort-pragmas": true,
            "flexible-oneline": true,
            "decl-no-blank-lines": ["where", "instance", "class"],
            "split-language-pragmas": true,
            "align-sum-type-decl": false,
            "sort-import-lists": true,
            "preserve-vertical-space": true,
            "sort-imports": true
        }
    },
    "language": "Haskell2010"
}
